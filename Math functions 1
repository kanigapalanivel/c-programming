#include <stdio.h>

#include <math.h>

int main() {

    float floatValue;

    int squareInput, base, power;

    // Reading input

    scanf("%f %d %d %d", &floatValue, &squareInput, &base, &power);

    // Applying math functions

    int floored = floor(floatValue);

    int ceiled = ceil(floatValue);

    int squareRoot = sqrt(squareInput);

    int powerResult = pow(base, power);

    // Printing output

    printf("%d\n", floored);

    printf("%d\n", ceiled);

    printf("%d\n", squareRoot);

    printf("%d\n", powerResult);

    return 0;

}
